{
  "name": "banksy",
  "version": "1.0.0",
  "description": "Bank transaction fetcher using selenium and publishing to SNS topic",
  "main": "handler.js",
  "scripts": {
    "test": "mocha -r ts-node/register ./test/*-spec.ts",
    "test:integration": "mocha -r ts-node/register ./integration-tests/*.ts --timeout 60000",
    "test:single": "mocha -r ts-node/register ./test/$test*-spec.ts --watch-extensions ts --w",
    "start": "ts-node ./src/index.ts",
    "build": "tsc",
    "credentials": "ts-node ./src/generateCredentials.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "aws-sdk": "^2.573.0",
    "chromedriver": "^79.0.0",
    "cryptr": "^6.0.1",
    "dotenv": "^8.2.0",
    "moment": "^2.24.0",
    "mongoose": "^5.7.12",
    "selenium-webdriver": "^4.0.0-alpha.5",
    "winston": "^3.2.1",
    "winston-aws-cloudwatch": "^3.0.0"
  },
  "devDependencies": {
    "@types/chai": "4.2.9",
    "@types/mocha": "7.0.1",
    "@types/mongoose": "5.7.2",
    "@types/selenium-webdriver": "4.0.6",
    "@types/sinon": "7.5.1",
    "chai": "4.2.0",
    "mocha": "7.1.0",
    "password-prompt": "1.1.2",
    "sinon": "9.0.0",
    "ts-node": "8.6.2",
    "typescript": "3.8.2"
  }
