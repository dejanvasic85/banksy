    "aws-sdk": "^2.573.0",
    "chromedriver": "^83.0.0",
    "cryptr": "^6.0.1",
    "dotenv": "^8.2.0",
    "moment": "^2.24.0",
    "pg": "^8.0.0",
    "selenium-webdriver": "^4.0.0-alpha.5",
    "winston": "^3.2.1",
    "winston-aws-cloudwatch": "^3.0.0",
    "ynab": "^1.18.0"
  },
  "devDependencies": {
    "@types/chai": "4.2.11",
    "@types/mocha": "7.0.2",
    "@types/pg": "7.14.4",
    "@types/selenium-webdriver": "4.0.9",
    "@types/sinon": "9.0.4",
    "chai": "4.2.0",
    "mocha": "8.0.1",
    "password-prompt": "1.1.2",
